<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ucsc.cmps121.ucscparking">

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:launchMode="singleTop"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainMenu"
            android:label="Menu" />
        <activity
            android:name=".AccountPrefs"
            android:label="Account Preferences" />
        <activity
            android:name=".PreferredLots"
            android:label="Preferred Lots" />
        <activity
            android:name=".PaymentSettings"
            android:label="Payment Settings" />
        <activity
            android:name=".PaymentHistory"
            android:label="Payment History" />
        <activity
            android:name=".MapList"
            android:label="Parking Maps" />
        <activity
            android:name=".ParkingMapView"
            android:label="Parking Map" />
        <activity
            android:name=".CameraActivity"
            android:label="Camera Test" />
    </application>

</manifest>